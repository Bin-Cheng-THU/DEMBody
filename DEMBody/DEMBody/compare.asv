clc;clear;
format long;
check = zeros(202,3);
for ii = 1:length(check)
    ii
    filename = num2str((ii-1)*0.0001,'%10.5f');
    filename = [filename,'Head.txt'];
    file1 = ['Lattice-Array\',filename];
    file2 = ['Lattice-Linklist\',filename];
    data1 = load(file1);
    data2 = load(file2);
    error = abs(data1-data2);
    check(ii,:) = sum(error);
end